{
  "name": "@fluidauth/express",
  "version": "1.0.0-alpha.7",
  "description": "A simple and flexible authentication solution for Express.js, making it easy to handle user authentication, sessions, and security features with minimal setup.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./base": {
      "require": "./dist/base/index.js",
      "import": "./dist/base/index.mjs",
      "types": "./dist/base/index.d.ts"
    },
    "./core": {
      "require": "./dist/core/index.js",
      "import": "./dist/core/index.mjs",
      "types": "./dist/core/index.d.ts"
    },
    "./providers": {
      "require": "./dist/providers/index.js",
      "import": "./dist/providers/index.mjs",
      "types": "./dist/providers/index.d.ts"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch ./server.ts --ext ts --exec tsx ./server.ts",
    "build": "tsup",
    "pub": "npm publish --access=public --scope=@fluidauth --tag alpha"
  },
  "author": "Imani Brown",
  "license": "ISC",
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/jsonwebtoken": "^9.0.6",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "nodemon": "^3.1.4",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "typescript",
    "authentication",
    "express",
    "session management",
    "security",
    "middleware",
    "user authentication"
  ],
  "repository": {
   "type": "git",
    "url": "https://github.com/Itszavier/fluidauth-express"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2"
  }
}
